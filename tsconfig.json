{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "baseUrl": "./",
        "paths": {
            "@commands/*": ["commands/*"],
            "@dictionary/*": ["dictionary/*"],
            "@enum/*": ["enum/*"],
            "@helper/*": ["helper/*"],
            "@lib/*": ["lib/*"],
        }
    },
    "include": [
        "**/*.ts"
    ]
}
